  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
      "url": "https://github.com/tomi/vscode-jsconfig.json/issues"
  },
  "homepage": "https://github.com/tomi/vscode-jsconfig.json/blob/master/README.md",
  "icon": "images/logo.png",
  "galleryBanner": {
    "color": "#ffffff",
    "theme": "light"
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "q": "^1.4.1"
  }
