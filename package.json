{
  "name": "create-jsconfig",
  "displayName": "Create jsconfig.json",
  "description": "Provides a command to create jsconfig.json with default content",
  "version": "0.1.1",
  "publisher": "TomiTurtiainen",
  "engines": {
    "vscode": "^0.10.7"
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "keywords": [
    "jsconfig.json"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.createJsconfig"
