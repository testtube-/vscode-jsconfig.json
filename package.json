{
  "name": "create-jsconfig",
  "displayName": "Create jsconfig.json",
  "description": "Provides a command and code action to create jsconfig.json with default content",
  "version": "0.0.1",
  "publisher": "TomiTurtiainen",
  "engines": {
    "vscode": "^0.10.1"
  },
  "keywords": [
    "jsconfig.json"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onCommand:extension.createJsconfig"
  ],
